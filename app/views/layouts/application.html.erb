<!DOCTYPE html>
<html>
<head>
  <title>Waitlist</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if notice %>
<p class="flash alert alert-success"><%= notice %></p>
<% end %>
<% if alert %>
<p class="flash alert alert-error"><%= alert %></p>
<% end %>
		<ul class="hmenu">
		  <%= render 'devise/menu/registration_items' %>
		  <%= render 'devise/menu/login_items' %>
		</ul>
		<%= yield %>
</body>
</html>
